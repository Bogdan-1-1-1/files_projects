#include "pch.h"
#include <iostream>
using namespace std;

static void move(int arr[], int size1, int n) {
	int help = 0;
	for (int i = 0; i < n; i++) {
	for (int j = size1 - 1; j > 0; j--) {
		help = *(arr + j);
		*(arr + j) = *(arr + j - 1);
		*(arr + j - 1) = help;
	}
}
}

int main()
{
	int n, size1;
	cin >> size1 >> n;
	int *arr = new int[size1];
	for (int i = 0; i < size1; i++) {
		arr[i] = i + 1;
		cout << arr[i] << " ";
	}
	move(arr, size1, n);
	cout << endl;
	for (int k = 0; k < size1; k++) {
		cout << *(arr + k) << " ";
	}
	return 0;
}
