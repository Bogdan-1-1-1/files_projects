#include <iostream>
using namespace std;

int main()
{
 int m,n,max1 = 0,i1,j1;
 cin >> n >> m;

int k[n][m];

 for(int i = 0; i < n;i++){
    for(int j =0; j < m; j++){
        cin >> k[i][j];
        if(max1 < k[i][j]) {
          max1 = k[i][j];
          i1 = i;
          j1= j;
        }
    }
 }
 cout << i1 << " " << j1;
}
